{
  "compilerOptions": {
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "ES2020",
    "downlevelIteration": true,
    "module": "Node16",
    "lib": ["ES6"],
    "outDir": "dist",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    // "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
